# Define the string containing the value inside the braces
$string = "Demo(12345)"

# Define the regular expression pattern to match the value inside the braces
$pattern = "\((.*?)\)"

# Use the Select-String cmdlet to find the value inside the braces
$value = $string | Select-String -Pattern $pattern | ForEach-Object { $_.Matches.Groups[1].Value }

# Print the value to the console
Write-Host $value
